{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "moduleResolution": "bundler",
        "strict": true,
        "paths": {
            "@/*": ["./src/*"],
            "@/assets/*": ["./assets/*"]
        }
    },
    "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
